QTableView {
	selection-background-color: rgb(220,230,220);
}

QMenu{
    background:white;
	border:1px solid lightgray;
}

QMenu::item{
	padding:0px 20px 0px 20px;
	height:25px;
}

QMenu::item:selected:enabled{
    background: #86BA10;
	color:white;
}

QMenu::separator{
    height:1px;
    background: lightgray;
	margin:2px 0px 2px 0px;
}

QMenu::item:selected:!enabled{
    background:transparent;
}

QLineEdit {
    border: 1px solid rgb(41, 119, 191);
    border-radius: 2px;
    padding: 0 5px;
    background: white;
    selection-background-color: blue;
}

QPushButton {
	border-radius: 2px;
	border: 1px solid rgb(41, 119, 191);
	background:transparent;
	color: rgb(41, 119, 191);
}

QPushButton:hover {
	background: rgb(41, 119, 191);
	color: white;
}

QTabWidget::pane{
	border:none;
}

QTabBar::tab{
	background:rgb(44, 122, 191);
	color:white;
	min-width:100px;
	min-height:25px;
}

QTabBar::tab:selected{
	border-color: white;
	background:white;
	color: rgb(41, 119, 191);
}

QLabel#WhiteLabel {
	color: white;
}

QComboBox {
	border: 1px solid rgb(33, 172, 227);
}

QComboBox QAbstractItemView::item {
	height: 25px;
	background: transparent;
}

QComboxBox::down-arrow {
	image: url(:/menu/arrow);
}

QComboBox::drop-down {
	width: 20px;
	border: none;
}

QHeaderView::section {
	background-color: rgb(41, 119, 191);
	color: white;
	padding-left: 4px;
	border: none;
	min-height: 25px;
}

